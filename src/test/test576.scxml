<?xml version="1.0" encoding="iso-8859-1"?><!-- test that the 'initial' value of scxml is respected.  We set the value to deeply nested non-default parallel siblings and
test that both are entered. --><scxml initial="s11p112 s11p122" datamodel="cplusplus" version="1.0" xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance">

     
<state id="s0">
  <transition target="fail"></transition>
</state>

<state id="s1">
  <onentry>
    <send event="timeout" delay="1s"></send>
    </onentry>
  <transition event="timeout" target="fail"></transition>
  <state id="s11" initial="s111">
    <state id="s111"></state>
    <parallel id="s11p1">
       <state id="s11p11" initial="s11p111">
         <state id="s11p111"></state>
         <state id="s11p112">
           <onentry>
             <raise event="In-s11p112"></raise>
             </onentry>
         </state>
       </state>
       <state id="s11p12" initial="s11p121">
        <state id="s11p121"></state>
        <state id="s11p122">
          <transition event="In-s11p112" target="pass"></transition>
          </state>
       </state>
      </parallel>
  </state>
</state>
 
<state id="pass" />
<state id="fail" />
</scxml>